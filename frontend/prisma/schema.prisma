generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model eomPrices {
  date DateTime
  ticker String
  price Float
  volume BigInt

  @@id([date, ticker])
  @@map("eom_prices")
}

model factorScores {
  date DateTime
  ticker String
  factor String
  score Float
  
  @@id([date, ticker, factor])
  @@map("factor_scores")
}

model monthlyConstituents {
  date DateTime
  ticker String
  sector String
  index_weight Float
  index String

  @@id([date, ticker])
  @@map("monthly_constituents")
}
